.onLoad <- function(...) {
packageStartupMessage(cat(paste("'qmethod' version ", packageVersion("qmethod")), "\n\nTo cite package 'qmethod' in publications, please use:\n\n    Aiora Zabala (2014) qmethod: A Package to Explore Human Perspectives\n    Using Q Methodology. The R Journal, 6(2):163-173"))
  }

